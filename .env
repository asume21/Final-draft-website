# CodedSwitch Environment Variables

# OpenAI API Key (required by OpenAI client library even when using xAI endpoint)
OPENAI_API_KEY=sk-dummy_value_for_xai_client

# xAI Grok API Key (your actual API key for Grok)
XAI_API_KEY=your_xai_api_key_here

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Database URL for Neon Database
DATABASE_URL=your_neon_database_url_here

# Node Environment
NODE_ENV=development